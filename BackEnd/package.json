    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.3",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.0",
    "tsc-watch": "^6.0.0",
    "typeorm": "^0.3.15",
    "typescript": "^5.0.4",
    "yarn": "^1.22.19"
